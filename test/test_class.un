struct number(x, y, z)

class point: number

    __len__ = fn() -> (self.x +
                       self.y + 
                       self.z) / 2
    __str__ = fn() -> `{self.x} {self.y} {self.z} {123}`
    new = fn() -> point(self.x, self.y, self.z)

    fn __init__(x, y, z)
        self.x = x
        self.y = y
        self.z = z

p1: point = point(55, 2, 348)
write(p1)