l = range(1, 10)

writeln(l)

l.remove(1)
l.remove(5)

writeln("1, 5 삭제")

writeln(l)

writeln("1번과 3번 값 삭제")
l.remove_at(1)
l.remove_at(3)

writeln(l)

writeln("값 : 1 존재 유무 : " + l.contains(1))
writeln("값 : 9 존재 유무 : " + l.contains(9))
writeln("값 : 0 존재 유무 : " + l.contains(0))

writeln("값 : 2의 위치 : " + l.index_of(2))
writeln("값 : 7의 위치 : " + l.index_of(7))
writeln("값 : 9의 위치 : " + l.index_of(9))

writeln("0번째 값을 99 변경")
l[0] = 99
writeln("1번째 값을 100 변경")
l[1] = 100
writeln("2번째 값을 101 변경")
l[2] = 101

writeln(l)

writeln("배열의 길이")
writeln(len(l))

writeln("더하기 연산 : 77")
l += 77
writeln(l)

writeln("더하기 연산 : 2.75")
l += 2.75
writeln(l)

writeln("더하기 연산 : true")
l += true
writeln(l)

writeln("더하기 연산 : 'hello'")
l += "hello"
writeln(l)

writeln("배열 1 add 함수 : 1")
l.add(1)
writeln(l)

writeln("clone 함수 => 배열 2")
l2 = l.clone()
writeln("배열 2 add 함수 : 2")
l.add(2)

writeln("배열 1")
writeln(l)
writeln("배열 2")
writeln(l2)