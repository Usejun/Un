import vector2

v1 = vector2(3, 2)
v2 = vector2(1, 6)
writeln("v1 : " + v1)
writeln("v2 : " + v2)
writeln("더하기 연산 : " + (v1 + v2))
writeln("빼기 연산 : " + (v1 - v2))
writeln("곱하기 연산 : " + v1 * v2)
writeln("나누기 연산 : " + v1 / v2)
writeln("정수 나누기 연산 : " + v1 // v2)
writeln("나머지 연산 : " + (v1 % v2))
writeln("OR 연산 : " + (v1 or v2))
writeln("AND 연산 : " + (v1 and v2))
writeln("XOR 연산 : " + (v1 xor v2))
writeln("Equal 연산 : " + (v1 == v2))
writeln("Unequal 연산 : " + (v1 != v2))
writeln("len 함수 : " + len(v1))
writeln("문자열 변환 : " + v1)
writeln("정수 변환 : " + int(v1))