d = dict()

for i in range(0, 5)
	d.add(i, i)

d.add("key", 123.1)
d.add(42.1, "##a")
d.add(true, "!@#123")

writeln("키:값 : " + d)
writeln("인덱싱 [1] : " + d[1])
writeln("인덱싱 ['key'] : " + d["key"])
writeln("인덱싱 [42.1] : " + d[42.1])
writeln("인덱싱 [true] : " + d[true])
writeln("keys 함수 : " + d.keys())
writeln("values 함수 유무 : " + d.values())
writeln("'key' 키 존재 유무 : " + d.contains_key("key"))
writeln("'key2' 키 존재 유무 : " + d.contains_key("key2"))
writeln("'xe' 값 존재 유무  : " + d.contains_value("xe"))
writeln("'##a' 값 존재 유무 : " + d.contains_value("##a"))
writeln("'key' 키 삭제  : " + d.remove("key"))
writeln("3 키 삭제  : " + d.remove(3))
writeln("모든 키 : " + d.keys())
writeln("모든 값 : " + d.values())